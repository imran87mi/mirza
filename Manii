def pilih():
	unikers = raw_input("\n\033[1;31;40m>>> \033[1;35;40m")
	if unikers =="":
		print "\x1b[1;92mFill in correctly"
		pilih()
	elif unikers =="1":
		super()
	elif unikers =="2":
		os.system('clear')
		print logo
		print " \033[1;36;40mâ—â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â—„â–ºâ•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â•â—\n"
		os.system('git pull origin master')
		raw_input('\n\x1b[1;92m[ \x1b[1;97mBack \x1b[1;92m]')
		menu()
	elif unikers =="0":
		jalan('Token Removed')
		os.system('rm -rf login.txt')
		keluar()
	else:
		print "\x1b[1;92mFill in correctly"
		pilih()

